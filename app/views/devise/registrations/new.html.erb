                <div id="side-text" class="col-md-4 pull-right">
            <h3><center><%= image_tag("logo.png") %></center></h3>
		<h1>Sign up</h1>

		<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
		  <%= devise_error_messages! %>
		  <div><p><%= f.label :name, "Full Name" %></p>
          <P><%= f.text_field :name, :autofocus => true %></p></div>

		  <div><p><%= f.label :email %></p>
		  <p><%= f.email_field :email, :autofocus => true %></p></div>

		  <div><p><%= f.label :password %></p>
		  <p><%= f.password_field :password %></p></div>

		  <div><P><%= f.label :password_confirmation %></p>
		  <p><%= f.password_field :password_confirmation %></p></div>

		  <div><p><%= f.submit "Sign up", :class => "btn" %></p></div>
		<% end %>

		<p>Already have an account? <%= render "devise/shared/links" %></p><br /><br />

					<!--<a href="index4.html">Contact Us</a>-->

